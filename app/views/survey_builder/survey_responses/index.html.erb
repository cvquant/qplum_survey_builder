<p id="notice"><%= notice %></p>

<h1>Listing Survey Responses</h1>

<table>
  <thead>
    <tr>
      <th>Survey form</th>
      <th>User</th>
      <th>Time to answer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_responses.each do |survey_response| %>
      <tr>
        <td><%= survey_response.survey_form %></td>
        <td><%= survey_response.user %></td>
        <td><%= survey_response.time_to_answer %></td>
        <td><%= link_to 'Show', survey_form_survey_response_path(@survey_form, @survey_response) %></td>
        <td><%= link_to 'Edit', edit_survey_form_survey_response_path(@survey_form, @survey_response) %></td>
        <td><%= link_to 'Destroy', survey_form_survey_response_path(@survey_form, @survey_response), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey response', new_survey_form_survey_response_path(@survey_form) %>
